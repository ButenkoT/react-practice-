{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS with hot code reloading, webpack, babel and karma",
  "scripts": {
    "start": "node server.js",
    "build": "webpack",
    "test": "node ./node_modules/karma/bin/karma start ./karma.conf.js --single-run --reporters nyan",
    "test:dev": "node ./node_modules/karma/bin/karma start ./karma.conf.js --no-single-run --auto-watch --reporters nyan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ButenkoT/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Tanya Butenko <tanya@butenko.me> (http://github.com/ButenkoT)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ButenkoT/react-boilerplate/issues"
  },
  "homepage": "https://github.com/ButenkoT/react-boilerplate",
  "devDependencies": {
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "css-loader": "^0.15.1",
    "jasmine": "^2.3.1",
    "json-loader": "^0.5.2",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-nyan-reporter": "0.1.0",
    "karma-webpack": "^1.5.1",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "lodash": "^3.10.0",
    "moment": "^2.10.3",
    "react": "^0.13.3"
  }
}
